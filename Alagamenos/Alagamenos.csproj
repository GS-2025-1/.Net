        <PackageReference Include="NSwag.Core" Version="14.4.0" />
        <PackageReference Include="Oracle.EntityFrameworkCore" Version="9.23.80" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Controllers\" />
    </ItemGroup>

